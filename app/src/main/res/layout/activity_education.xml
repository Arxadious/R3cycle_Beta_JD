<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/light_green">


    <Button
        android:id="@+id/top"
        android:layout_width="438dp"
        android:layout_height="81dp"
        android:background="@drawable/rounded_button_background"
        android:drawableStart="@drawable/ic_community"
        android:drawablePadding="0dp"
        android:paddingStart="80dp"
        android:text="Education"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/topDarkBar"
        android:layout_width="413dp"
        android:layout_height="23dp"
        android:layout_marginTop="80dp"
        android:background="@color/dark_green"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/textView7"
        android:layout_width="293dp"
        android:layout_height="102dp"
        android:layout_marginTop="364dp"
        android:layout_marginEnd="48dp"
        android:text="@string/educationtext3"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="287dp"
        android:layout_height="328dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="176dp"
        android:text="@string/educationtext"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="287dp"
        android:layout_height="328dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="272dp"
        android:text="@string/educationtext2"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/titleText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="128dp"
        android:layout_marginEnd="148dp"
        android:text="@string/r3cycle"
        android:textColor="@color/dark_blue"
        android:textSize="32sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/topLightGreenBar"
        android:layout_width="422dp"
        android:layout_height="24dp"
        android:layout_marginTop="100dp"
        android:background="@color/green"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/bottomDarkBar"
        android:layout_width="420dp"
        android:layout_height="23dp"
        android:layout_marginBottom="76dp"
        android:background="@color/dark_green"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent">

    </LinearLayout>


    <LinearLayout
        android:id="@+id/bottomLightGreenBar"
        android:layout_width="426dp"
        android:layout_height="24dp"
        android:layout_marginBottom="20dp"
        android:background="@color/green"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="@+id/bottomDarkBar"
        app:layout_constraintEnd_toEndOf="parent">

    </LinearLayout>

    <Button
        android:id="@+id/homeButton"
        android:layout_width="411dp"
        android:layout_height="76dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="656dp"
        android:background="@drawable/rounded_button_background"
        android:drawableStart="@drawable/left_arrow"
        android:drawablePadding="8dp"
        android:paddingStart="20dp"
        android:text="Resources"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="340dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/rectanglelong" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="103dp"
        android:layout_height="19dp"
        android:layout_marginEnd="136dp"
        android:layout_marginBottom="348dp"
        android:text="Helpful links"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="310dp"
        android:layout_height="98dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="220dp"
        android:text="https://www.orlando.gov/Trash-Recycling"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="310dp"
        android:layout_height="98dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="184dp"
        android:text="https://www.orlando.gov/Trash-Recycling/Food-Waste"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="310dp"
        android:layout_height="98dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="136dp"
        android:text="https://www.orlando.gov/Trash-Recycling/What-Goes-Where"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>